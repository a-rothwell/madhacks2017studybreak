{
  

  "manifest_version": 2,

  "name": "StudyBreak",
  "description": "extension that lets you have fun during studying",
  "options_page": "options.html",
  "version": "1",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
   "default_icon": "studyBreak.png",
   "default_popup": "popup.html"

 },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "activeTab",
    "tabs"
  ]
}
